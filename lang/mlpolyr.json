		"BRACE": "BRACES",
		"BRACK": "BRACKETS",
		"SEMICOLON": "SEMICOLON",
		"UNRESOLVED": "IDENTIFIER",
		"OPERATOR": "OPERATION_SIGN",
		"BRACE2": "BRACES",
		"COMMENT": "BLOCK_COMMENT",
		"DOT": "DOT",
		"INT": "NUMBER",
		"STRING": "STRING",
		"FUNCTION_CALL": "FUNCTION_CALL",
		"FUNCTION_DECL": "FUNCTION_DECLARATION",
		"VARIABLE_CALL": "GLOBAL_VARIABLE",
		"VARIABLE_DECL": "GLOBAL_VARIABLE",
		"PATTERN_CALL": "LOCAL_VARIABLE",
